<?xml version="1.0" ?>
<launch>
  <param name="robot_description" command="xacro '$(find thormang3_description)/urdf/thormang3.xacro'" />

  <!-- send joint values -->
  <node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher">
    <!--<rosparam param="source_list">["robotis/real_joint_states"]</rosparam>-->
  </node>

  <!-- Combine joint values -->
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"/>

  <!-- Show in Rviz -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find thormang3_description)/rviz/thormang3.rviz"/>

</launch>
