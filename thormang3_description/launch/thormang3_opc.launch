<?xml version="1.0" ?>
<launch>
  <!-- Robot Model -->
  <!--param name="robot_description" command="xacro '$(find thormang3_description)/urdf/thormang3.xacro'" /-->

  <!-- Show joint values -->
  <node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher">
    <remap from="/joint_states" to="/opc/thormang3/joint_states" />
    <rosparam param="source_list">["/robotis/thormang3/joint_states"]</rosparam>
  </node>

  <!-- Show in Rviz -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find thormang3_description)/rviz/thormang3_display.rviz" output="screen"/>
</launch>
